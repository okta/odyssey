{
  "name": "@okta/ui-shell",
  "version": "1.25.0",
  "description": "UI Shell package with no dependencies.",
  "author": "Okta, Inc.",
  "license": "Apache-2.0",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/src/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/src/index.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/ui-shell.es.d.ts",
      "default": "./dist/ui-shell.es.js"
    },
    "./README.md": "./README.md"
  },
  "scripts": {
    "build:clean": "rimraf dist",
    "build:ui-shell": "vite build",
    "build:types": "tsc --project tsconfig.production.json",
    "build": "yarn build:clean && yarn build:types && yarn build:ui-shell",
    "dev:types": "yarn build:types --watch",
    "dev": "yarn concurrently npm:dev:source npm:dev:types",
    "lint": "eslint .",
    "prepack": "yarn exec prepack",
    "test": "jest",
    "typecheck": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/okta/odyssey",
    "directory": "packages/ui-shell"
  },
  "devDependencies": {
    "@happy-dom/jest-environment": "^15.7.4",
    "@okta/odyssey-icons": "workspace:*",
    "@okta/odyssey-lifecycle": "workspace:*",
    "@okta/odyssey-react-mui": "workspace:*",
    "@okta/odyssey-typescript": "workspace:*",
    "@types/jest-axe": "^3.5.9",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.3.2",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "jest-axe": "^5.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "regenerator-runtime": "^0.14.1",
    "rimraf": "^5.0.1",
    "tsx": "^4.7.3",
    "typescript": "^5.5.4",
    "vite": "^5.4.9",
    "vite-plugin-dts": "^4.2.4"
  }
}
