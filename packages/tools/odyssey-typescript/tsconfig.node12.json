{
  "extends": "./tsconfig.strict.json",

  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "emitDeclarationOnly": false,
    "lib": ["es2019", "es2020.bigint", "es2020.promise", "es2020.string"],
    "module": "CommonJS",
    "moduleResolution": "Node",
    "noEmit": false,
    "target": "es2019"
  }
}
