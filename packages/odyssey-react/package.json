{
  "name": "@okta/odyssey-react",
  "version": "0.11.3",
  "description": "React components for Odyssey, Okta's design system",
  "author": "Okta, Inc.",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "default": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/okta/odyssey",
    "directory": "packages/odyssey-react"
  },
  "dependencies": {
    "@okta/odyssey-react-theme": "^0.11.3",
    "@react-aria/focus": "3.5.0",
    "choices.js": "^9.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@okta/browserslist-config-odyssey": "^0.11.3",
    "@okta/odyssey-babel-preset": "^0.11.3",
    "@okta/odyssey-icons": "^0.11.3",
    "@okta/odyssey-postcss-preset": "^0.11.3",
    "@okta/odyssey-postcss-scss": "^0.11.3",
    "@okta/odyssey-stylelint": "^0.11.3",
    "@okta/odyssey-svgr": "^0.11.3",
    "@okta/odyssey-typescript": "^0.11.3",
    "@svgr/cli": "^5.5.0",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest-axe": "^3.5.1",
    "@types/react": "^17.0.30",
    "@types/react-dom": "^17.0.5",
    "babel-jest": "^26.6.3",
    "chokidar": "^3.4.0",
    "jest": "^26.6.3",
    "jest-axe": "^5.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "regenerator-runtime": "^0.13.7"
  },
  "peerDependencies": {
    "react": ">=16 <18",
    "react-dom": ">=16 <18"
  },
  "scripts": {
    "start": "node ./scripts/start.cjs",
    "lint": "eslint .",
    "test": "jest",
    "typecheck": "tsc",
    "prepare": "yarn build",
    "build": "yarn build:clean && yarn build:types && yarn build:source",
    "build:clean": "rm -rf dist",
    "build:types": "tsc --project tsconfig.production.json",
    "build:source": "./scripts/buildSource",
    "build-icons": "svgr $(node ./scripts/resolveIconSrcPath.cjs) --out-dir ./src/components/Icon/",
    "prepack": "prepack"
  }
}
