{
  "name": "@okta/odyssey-contribution-tooling",
  "version": "1.52.1",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.es.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.cjs",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "yarn ok-ui build",
    "dev:build-source": "yarn vite build --watch",
    "dev:build-types": "yarn ok-ui build --watch",
    "dev": "yarn concurrently npm:dev:build-source npm:dev:build-types",
    "playwright-install": "yarn playwright install --with-deps chromium",
    "provision": "yarn ok-ui provision",
    "test": "yarn ok-ui test",
    "typecheck": "yarn ok-ui typecheck"
  },
  "dependencies": {
    "i18next": "^23.15.1",
    "react-i18next": "15.4.1"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@okta/ok-ui": "^4.15.1",
    "@okta/ui-stacks-component-libraries-odyssey-contributions": "1.1.3",
    "@testing-library/dom": "^10.4",
    "@testing-library/jest-dom": "^6.8",
    "@testing-library/react": "^16.3",
    "@testing-library/user-event": "14.x",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^5.0",
    "@vitest/browser": "^3.2",
    "@vitest/coverage-v8": "^3.2",
    "@vitest/ui": "^3.2",
    "concurrently": "^9.2.1",
    "jest-axe": "^10.0",
    "jsdom": "^26.1",
    "msw": "^2.10.5",
    "playwright": "^1.57.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "regenerator-runtime": "^0.14.1",
    "typescript": "^5.9.2",
    "vite": "^7.1",
    "vite-plugin-svgr": "^4.5",
    "vitest": "^3.2"
  }
}
