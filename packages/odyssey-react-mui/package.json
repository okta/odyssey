{
  "name": "@okta/odyssey-react-mui",
  "version": "0.15.0",
  "description": "React MUI components for Odyssey, Okta's design system",
  "author": "Okta, Inc.",
  "license": "Apache-2.0",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "default": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/okta/odyssey",
    "directory": "packages/odyssey-react-mui"
  },
  "dependencies": {
    "@okta/odyssey-design-tokens": "workspace:*"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.3",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.3",
    "@mui/material": "^5.10.5",
    "@okta/babel-plugin-fully-specified": "workspace:*",
    "@okta/browserslist-config-odyssey": "workspace:*",
    "@okta/odyssey-babel-plugin": "workspace:*",
    "@okta/odyssey-babel-preset": "workspace:*",
    "@okta/odyssey-icons": "workspace:*",
    "@okta/odyssey-lifecycle": "workspace:*",
    "@okta/odyssey-svgr": "workspace:*",
    "@okta/odyssey-typescript": "workspace:*",
    "@svgr/cli": "^6.3.1",
    "@testing-library/dom": "^7.28.1",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/babel__preset-env": "^7",
    "@types/jest-axe": "^3.5.1",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.5",
    "babel-plugin-import": "^1.13.5",
    "eslint": "^8.24.0",
    "jest": "^29.0.3",
    "jest-axe": "^5.0.1",
    "jest-environment-jsdom": "^29.0.3",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "regenerator-runtime": "^0.13.7",
    "stylelint": "^14.13.0",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@mui/icons-material": "^5",
    "@mui/material": "^5.9.0",
    "react": ">=17 <19",
    "react-dom": ">=17 <19"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "jest",
    "typecheck": "tsc",
    "build": "npm-run-all --sequential build:clean --parallel build:types build:source",
    "build:clean": "rm -rf dist",
    "build:types": "tsc --project tsconfig.production.json",
    "build:source": "NODE_ENV=production BROWSERSLIST_ENV=modern babel src --out-dir dist --extensions .js,.jsx,.ts,.tsx --source-maps",
    "build:icons": "svgr ../odyssey-icons/src --out-dir ./src/components/Icon",
    "dev": "npm-run-all --sequential dev:clean --parallel dev:types dev:source",
    "dev:clean": "yarn build:clean",
    "dev:types": "yarn build:types --watch",
    "dev:source": "yarn build:source --watch",
    "prepack": "yarn exec prepack"
  }
}
