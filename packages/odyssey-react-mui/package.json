{
  "name": "@okta/odyssey-react-mui",
  "version": "1.30.0",
  "description": "React MUI components for Odyssey, Okta's design system",
  "author": "Okta, Inc.",
  "license": "Apache-2.0",
  "sideEffects": false,
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/src/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/src/index.d.ts"
      ],
      "icons": [
        "./dist/src/icons.generated/index.d.ts"
      ],
      "labs": [
        "./dist/src/labs/index.d.ts"
      ],
      "test-selectors": [
        "./dist/src/test-selectors/index.d.ts"
      ],
      "ui-shell": [
        "./dist/src/ui-shell/index.d.ts"
      ],
      "web-component": [
        "./dist/src/web-component/index.d.ts"
      ]
    }
  },
  "exports": {
    "./icons": {
      "types": "./dist/src/icons.generated/index.d.ts",
      "default": "./dist/src/icons.generated/index.js"
    },
    "./labs": {
      "types": "./dist/src/labs/index.d.ts",
      "default": "./dist/src/labs/index.js"
    },
    "./test-selectors": {
      "types": "./dist/src/test-selectors/index.d.ts",
      "default": "./dist/src/test-selectors/index.js"
    },
    "./ui-shell": {
      "types": "./dist/src/ui-shell/index.d.ts",
      "default": "./dist/src/ui-shell/index.js"
    },
    "./web-component": {
      "types": "./dist/src/web-component/index.d.ts",
      "default": "./dist/src/web-component/index.js"
    },
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./dist/src/index.js"
    },
    "./README.md": "./README.md"
  },
  "scripts": {
    "build:clean": "rimraf dist",
    "build:source": "tsc --project tsconfig.production.json",
    "build:translationsJson": "tsx scripts/properties-to-ts.ts bundle",
    "build": "yarn build:clean && yarn build:translationsJson && yarn build:source",
    "ci:coverage": "NODE_ENV=test yarn vitest run --coverage",
    "dev:coverage": "NODE_ENV=test yarn vitest --coverage",
    "dev:source": "tsc --project tsconfig.json --watch",
    "dev:test": "NODE_ENV=test vitest",
    "dev:translationsJson": "build:translationsJson --watch",
    "dev": "yarn concurrently npm:dev:translationsJson npm:dev:source",
    "generate:icons:clean": "rimraf src/icons.generated && rimraf src/icons.generated/index.ts",
    "generate:icons:components": "svgr ../odyssey-icons/dist/icons --out-dir ./src/icons.generated",
    "generate:icons:index": "tsx scripts/generateIconsIndex.ts",
    "generate:icons": "yarn generate:icons:clean && yarn generate:icons:components && yarn generate:icons:index",
    "test": "NODE_ENV=test vitest run",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/okta/odyssey",
    "directory": "packages/odyssey-react-mui"
  },
  "dependencies": {
    "@dnd-kit/core": "6.0.3",
    "@dnd-kit/sortable": "7.0.0",
    "@dnd-kit/utilities": "3.2.0",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^5.16.14",
    "@mui/lab": "^5.0.0-alpha.175",
    "@mui/material": "^5.16.14",
    "@mui/system": "^5.16.14",
    "@mui/utils": "^5.16.14",
    "@mui/x-date-pickers": "^7.2.0",
    "@okta/odyssey-design-tokens": "workspace:^",
    "@types/luxon": "^3.4.2",
    "@vitest/browser": "^2.1.5",
    "@vitest/ui": "^2.1.5",
    "date-fns": "^2.30.0",
    "dom-accessibility-api": "^0.7.0",
    "i18next": "^23.15.1",
    "luxon": "^3.4.4",
    "material-react-table": "^2.11.3",
    "react-error-boundary": "^4.1.1",
    "react-i18next": "^15.1.1",
    "react-window": "^1.8.10",
    "word-wrap": "^1.2.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@happy-dom/jest-environment": "^15.7.4",
    "@okta/browserslist-config-odyssey": "workspace:*",
    "@okta/odyssey-icons": "workspace:*",
    "@okta/odyssey-typescript": "workspace:*",
    "@svgr/cli": "^7.0.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest-axe": "^3.5.9",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-virtualized-auto-sizer": "^1.0.4",
    "@types/react-window": "^1.8.8",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "@vitest/coverage-v8": "^2.1.8",
    "babel-plugin-import": "^1.13.5",
    "concurrently": "^8.2.2",
    "jest-axe": "^5.0.1",
    "properties": "1.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "regenerator-runtime": "^0.14.1",
    "rimraf": "^5.0.1",
    "tsd": "^0.31.2",
    "tsx": "^4.7.3",
    "typescript": "^5.6.3",
    "vite": "^6.0.7",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.5",
    "yargs": "^17.7.2"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
